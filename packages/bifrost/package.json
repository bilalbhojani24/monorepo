{
  "name": "@browserstack/bifrost",
  "version": "0.4.9",
  "main": "dist/index.js",
  "description": "UI Packages for Browserstack Frontend",
  "scripts": {
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "chromatic": "npx chromatic --project-token=e55097a5c9b3",
    "build": "vite build",
    "lint:check": "eslint --format=table --ignore-pattern dist --max-warnings 0 \"**/*.{js,jsx}\"",
    "lint:fix": "npm run lint:check -- --fix",
    "lint:fix:modified": "eslint --fix --max-warnings 0",
    "stylelint:check": "stylelint --formatter=verbose \"**/*.{css,scss}\"",
    "stylelint:fix": "npm run stylelint:check -- --fix",
    "stylelint:fix:modified": "stylelint --fix",
    "prettier:check": "prettier --check \"**/*.{js,jsx}\" \"**/*.{css,scss}\"",
    "prettier:fix": "npm run prettier:check -- --write",
    "tailwind-preview": "tailwind-config-viewer -o"
  },
  "dependencies": {
    "@headlessui/react": "1.7.5",
    "@heroicons/react": "2.0.13",
    "@radix-ui/react-popover": "^1.0.2",
    "@radix-ui/react-tooltip": "1.0.2",
    "mini-css-extract-plugin": "^2.7.2",
    "prettier": "^2.8.2",
    "classnames": "^2.2.6",
    "tailwind-merge": "^1.8.1"
  },
  "peerDependencies": {
    "prop-types": "15.8.1",
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "devDependencies": {
    "@browserstack/stylelint-config": "workspace:*",
    "@browserstack/tailwind-config": "workspace:*",
    "@browserstack/vite-config": "workspace:*",
    "@storybook/addon-a11y": "6.5.14",
    "@storybook/addon-actions": "^6.1.20",
    "@storybook/addon-docs": "6.4.19",
    "@storybook/addon-essentials": "^6.1.20",
    "@storybook/addon-links": "^6.1.20",
    "@storybook/builder-webpack5": "6.5.14",
    "@storybook/manager-webpack5": "6.5.14",
    "@storybook/node-logger": "^6.1.20",
    "@storybook/preset-create-react-app": "^3.1.6",
    "@storybook/react": "^6.1.20",
    "@tailwindcss/forms": "0.5.3",
    "@tailwindcss/nesting": "0.0.0-insiders.565cd3e",
    "@vitejs/plugin-react": "^3.0.0",
    "component-file-generator": "0.5.2",
    "css-loader": "6.7.2",
    "postcss": "8.4.20",
    "postcss-loader": "7.0.2",
    "postcss-preset-env": "^7.8.3",
    "sass": "^1.57.1",
    "storybook-addon-sass-postcss": "^0.1.3",
    "style-loader": "^3.3.1",
    "stylelint": "^14.16.0",
    "stylelint-config-prettier": "^9.0.4",
    "stylelint-config-standard-scss": "latest",
    "stylelint-prettier": "^2.0.0",
    "stylelint-scss": "^4.3.0",
    "tailwind-config-viewer": "^1.7.2",
    "tailwindcss": "^3.1.4",
    "vite": "^4.0.0"
  },
  "files": [
    "dist/*"
  ],
  "sideEffects": false
