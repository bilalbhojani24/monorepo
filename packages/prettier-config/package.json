{
  "name": "@browserstack/prettier-config",
  "version": "0.1.0",
  "main": "index.js",
  "license": "MIT",
  "peerDependencies": {
    "prettier": "2.8.1",
    "prettier-plugin-css-order": "^1.3.0",
    "prettier-plugin-tailwindcss": "^0.2.1"
  },
  "postcss": {
    "syntax": "postcss-scss",
