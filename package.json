{
  "name": "frontend",
  "version": "0.1.0",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "start": "turbo run start",
    "storybook": "turbo run storybook",
    "build": "turbo run build",
    "dev": "turbo run dev --parallel",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\""
  },
  "devDependencies": {
    "bifrost": "workspace:*",
    "eslint-config-browserstack": "workspace:*",
    "stylelint-bs-config": "workspace:*",
    "tailwind-bs-config": "workspace:*",
    "prettier-bs-config": "workspace:*",
    "turbo": "latest"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "dependencies": {},
  "packageManager": "pnpm@7.18.2"
}
